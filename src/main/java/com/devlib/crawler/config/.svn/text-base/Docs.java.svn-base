package com.devlib.crawler.config;

/**
 * item > docs <br>
 * 一个页面有多个同级数据的情况，一个既是SCD文件中的一个文档
 * 
 * @author wenqing zhu
 * 
 */
public class Docs extends HasItems {
	String currentPageXpath;

	String maxPageNum;

	public String getCurrentPageXpath() {
		return currentPageXpath;
	}

	public void setCurrentPageXpath(String currentPageXpath) {
		this.currentPageXpath = currentPageXpath;
	}

	public String getMaxPageNum() {
		return maxPageNum;
	}

	public void setMaxPageNum(String maxPageNum) {
		this.maxPageNum = maxPageNum;
	}

}
